{
    "ApplicationSettings": {
        "ApplicationTitle": "ClientTools",
        "DefaultDatabaseEngine": "PostgreSQLEngine",
        "ValidEngineValues_Comment": "SQLiteEngine | PostgreSQLEngine | SqlServerEngine",
        "LogFileFolder": "Logs",
        "Log.Critical": "critical-dev.log",
        "Log.Debug": "debug-dev.log",
        "Log.Error": "error-dev.log",
        "Log.Information": "information-dev.log",
        "Log.Trace": "trace-dev.log",
        "Log.Warning": "warning-dev.log",
        "Logging.Enabled": "true",
        "Breaks": "[\u000A\u000B\u000C\u000D\u2028\u2029\u0085]+",
        "FeedLines": "(?:\r\n|\r(?!\n)|(?<!\r)\n){3,}",
        "UserAgentHeader": "Mozilla/5.0 (Windows NT 6.1) AppleWebKit/537.36 (KHTML, like Gecko)  Chrome/41.0.2228.0 Safari/537.36",
        "InlineTags": "b,big,i,small,intott,abbr,acronym,cite,code,dfn,em,kbd,strong,samp,var,a,bdo,br,img,map,object,q,script,span,sub,sup,button,input,label,select,textarea,font"
        "ContainerTags": "table,ul,p,div",
        "NonDisplayTags": "script,style",
        "ItemTags":  "li,tr"
    },
    "DatabaseSettings": {
        "SqlServer": {
            "DataSource": "SQLSERVERNAME_OR_INSTANCE",
            "InitialCatalog": "YourInitialCatalog",
            "IntegratedSecurity": false,
            "UserID": "userid",
            "Password": "password",
            "ApplicationName": "ApplicationName",
            "TimeOut": "100",
            "ConnectionPooling": "true"
        },
        "SQLite": {
            "SQLitePath": "Repository",
            "SQLiteFileName": "YourSQLiteName.db"
        },
        "PostgreSQL": {
            "HOST": "yourHost.amazonaws.com",
            "UserName": "yourusername",
            "DatabaseName": "yourdatabasename",
            "Port": "5432",
            "Password": "YourPassword",
            "PersistSecuritInfo": "false",
            "IntegratedSecurity": "true",
            "SearchPath": "public",
            "TimeOut": "100",
            "ApplicationName": "Test",
            "TrustCertificate": "true",
            "SSLMode": "1",
            "ValidSSLValues_Comment": "Disable=0 Prefer=1 Require=2"
        }
    }
}
